/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "noImplicitAny": false, // This is the line that I added to fix the error
    "strictPropertyInitialization": false, // This is the line that I added to fix the error
    "strictNullChecks": false, // This is the line that I added to fix the error
    "noImplicitReturns": false, // This is the line that I added to fix the error
    "noPropertyAccessFromIndexSignature": false, // This is the line that I added to fix the error
    "noImplicitOverride": false, // This is the line that I added to fix the error
    "types": []
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
